generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Course {
  id              Int      @id @default(autoincrement())
  course_name     String
  section_id      String
  instructor      String
  total_seats     Int
  open_seats      Int
  waitlist_count  Int
  class_times     Json
  last_updated    DateTime @default(now())
  custom_course_id String  @unique
  removed         Boolean  @default(false)
}